{% block sw_cms_block_image_multiple_grid %}
	<div class="sw-cms-block-image-multiple-grid">
		<div class="sw-cms-block-image-multiple-grid__top">
			<slot name="top"></slot>
		</div>
		<div class="sw-cms-block-image-multiple-grid__left-bottom">
			<slot name="left-bottom"></slot>
		</div>
		<div class="sw-cms-block-image-multiple-grid__right-bottom">
			<slot name="right-bottom"></slot>
		</div>
		<div class="sw-cms-block-image-multiple-grid__left-top">
			<slot name="left-top"></slot>
		</div>
		<div class="sw-cms-block-image-multiple-grid__right-top">
			<slot name="right-top"></slot>
		</div>
		<div class="sw-cms-block-image-multiple-grid__bottom">
			<slot name="bottom"></slot>
		</div>
	</div>
{% endblock %}
